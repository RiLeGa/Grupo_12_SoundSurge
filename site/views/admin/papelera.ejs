<!DOCTYPE html>
<html lang="es">
    <%- include ("../partials/head",{title: "Papelera"}) %>
<body class="home">
    <%- include ("../partials/header") %> 
    <main>
        <div class="categorias-home">
            <h3 class="secciones"> Eliminados </h3>
        </div>
        <section class="contenedor">
            <ul>
                <% for( let i = 0; i <= historial.lenght ; i++ ) { %>
                    <article>
                        <div class="producto"  style="background-image:url('./images/<%= historial[i].imagenes %>')">
                            <div class="precio-1">
                                <h3><%= historial.titulo %></h3>
                                <span >$<%= historial.precio %></span>
                            </div>
                        </div>
                    </article>
                <% } %>
            </ul>
    </main>
</body>
</html>