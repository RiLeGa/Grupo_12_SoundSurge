<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head', {title: 'Busqueda'}) %>

  <body>
    <%- include('./partials/header') %>
    <main>
      <% if (resultados.length != 0) { %>
        <ul>
          <% for( let i = 0; i <= resultados.length ; i++ ) { %>
              <div><%- resultados.nombre %> </div>
          <% } %>
        </ul>
      <% }else { %>
      <h1>
        No se encontraron coincidencias con la busqueda de: <%= busqueda %>
      </h1>
      <section class="resultados">
        <div class="card">
          <div class="card-img">
            <img src="/images/searchNotFound.png" alt="producto no encontrado" />
          </div>
          <div class="card-text-box">
            <h3 class="card-name">No se entraron productos</h3>
          </div>
        </div>
      </section>
      <% } %>
    </main>
    <%- include('./partials/footer') %>
    
  </body>
</html>